<!DOCTYPE html>
<html>
<%- include('./layouts/head'); -%>
<body>
<div class="container">
    <table class='table'>
        <tr>
            <td><strong>Title</strong></td>
            <td><strong>Price</strong></td>
            <td><strong>Stock</strong></td>
        </tr>
        <% products.forEach(function(product){ %>
            <tr>
                <td><%=product.title%></td>
                <td><%=product.price%></td>
                <td><%=product.stock%></td>
                <td><button>Add to cart</button></td>
            </tr>
        <% }); %>
    </table>
</div>
<%- include('./layouts/footer'); -%>
<!--<script src="static/javascripts/products.js"></script>-->
</body>
</html>